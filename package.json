{
    "name": "AgriCulture_Export_Information",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/audit-logging": "^5.6.2",
        "@sap/cds": "^6",
        "@sap/xsenv": "^3.4.0",
        "@sap/xssec": "^3.2.14",
        "express": "^4",
        "hdb": "^0.19.0",
        "passport": "^0.6.0"
    },
    "devDependencies": {
        "sqlite3": "^5.0.4",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "start": "cds run",
        "undeploy": "cf undeploy AgriCulture_Export_Information --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
    },
    "engines": {
        "node": "^16.15"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "requires": {
            "[production]": {
                "uaa": {
                    "kind": "xsuaa"
                }
            },
            "auth": {
                "[production]": {
                    "strategy": "JWT"
                }
            },
            "db": {
                "kind": "sql"
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        }
    }
}